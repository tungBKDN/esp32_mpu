void displayBatteryLevel(float voltage) {
    
    // Select the appropriate battery icon based on voltage range
    if (voltage > 3.3) {
      drawBattery(image_data_fullbattery);
      Serial.println("Battery status: FULL");
    } 
    else if (voltage >= 3.2) {
      drawBattery(image_data_mediumbattery);
      Serial.println("Battery status: MEDIUM");
    }
    else if (voltage >= 3.1 ) {
      drawBattery(image_data_lowbattery);
      Serial.println("Battery status: LOW");
    }
    else if (voltage >= 3.0) {
      drawBattery(image_data_outbattery);
      Serial.println("Battery status: CRITICAL");
    }
    else {
      // For voltages below 3.5V, also display out battery icon
      drawBattery(image_data_outbattery);
      Serial.println("Battery status: DEPLETED");
    }
    
  }